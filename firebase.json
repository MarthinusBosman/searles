{
	"hosting": {
		"public": "public",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"function": "searles"
			}
		],
		"predeploy": ["npm run build"]
	},
	"functions": {
		"predeploy": ["npm install", "npm run build"],
		"source": "functions"
	}
}
